class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int curr = 0;
        set<int> st;
        for (int i = 0; i < nums.size(); ++i) {
            if (st.insert(nums[i]).second == false) {
                return nums[i];
            }
        }
        return nums[0];
    }
};
